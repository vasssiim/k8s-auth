apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: micro-auth-ingress
  namespace: micro-auth
  annotations:
    kubernetes.io/ingress.class: nginx
spec:
  rules:
    - host: auth.localtest.me
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: web-frontend
                port:
                  number: 80

